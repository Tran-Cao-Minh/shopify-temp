<script src="{{ 'aos.js' | asset_url }}"></script>
<script>
    AOS.init({
        disable: "phone",
        delay: 0,
        duration: 1000
    });

    $(document).ready(function() {
        // Section 3 brandFan popup

        // Section 7 video
        let myVideo = document.getElementById("play_video_1");

        $(document).on('click', '[data-play_video_1]', function () {
            $(this).hide();

            if (myVideo.paused) {
                myVideo.play();
                $(this).hide();
            }else {
                myVideo.pause();
                $(this).hide();
            }
        });

        $(document).on('click', '[data-video_popup1]', function (e) {
            e.preventDefault();
            const urlYT = $(this).data('url');
            $('#iframe_video1').attr("src", urlYT);
            $('.smart_modal-video').show();
        });

        $(document).on('click', '[data-close_video]', function (e) {
            e.preventDefault();
            $('#iframe_video1').attr("src", "");
            $('.smart_modal').hide();
        });

    });
</script>

