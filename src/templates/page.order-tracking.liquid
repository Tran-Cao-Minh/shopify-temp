{{ 'order-tracking.css' | asset_url | stylesheet_tag }}
<div class="page_order-tracking" data-page_border>
    <div class="container">
        <div class="section-header text-center">
            <h1>{{ page.title }}</h1>
        </div>
        <div class="rte">
            {{ page.content }}
        </div>
        <div class="order__tracking">
            <div class="order__tracking--content">
                <form id="order__tracking--form" class="order__tracking--form form-vertical" method="GET">
                    <div class="form-group">
                        <div class="group_label">
                            <label for="order_id">{{ 'order-tracking.content.order_id_label' | t }}</label>
                            <div class="smart_tooltip">
                                {% include 'icon-info' %}<span class="tooltiptext">{{ 'order-tracking.content.order_id_tooltip_text' | t }}</span>
                            </div>
                        </div>
                        <input type="text" id="order_id" class="order__id form-control" name="order_id" required>
                        <div class="order_id--error"></div>
                    </div>
                    <div class="form-group">
                        <div class="group_label">
                            <label for="contact">{{ 'order-tracking.content.contact_label' | t }}</label>
                            <div class="smart_tooltip mb_right">
                                {% include 'icon-info' %}<span class="tooltiptext">{{ 'order-tracking.content.email_mobile_tooltip_text' | t }}</span>
                            </div>
                        </div>
                        <input id="contact" type="text" class="contact" name="contact" required>
                        <div class="contact--error"></div>
                        <div class="form-note">{{ 'order-tracking.content.hint' | t }}</div>
                    </div>
                    <div class="form-actions group-button d-flex">
                        <div class="order__tracking--result">
                            <h4 class="tracking__label"></h4>
                            <div class="d-flex">
                                <div class="container__icon"></div>
                                <div class="container__status-result">
                                    <span class="order__status"></span>
                                    <span class="tracking__url"></span>
                                </div>
                            </div>
                        </div>
                        <div class="form__actions-sticky">
                            <button class="btn track__order">{{ 'order-tracking.content.submit' | t }}<div class="loader" style="display: none;"></button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


{% assign contact_info = shop.metafields.data.contact_info %}
{% assign not_found_text = 'order-tracking.result.not_found_html' | t %}
{% if contact_info.contact_phone != blank %}
    {% assign not_found_text = not_found_text | replace: "${phone}", contact_info.contact_phone %}
{% else %}
    {% assign not_found_text = not_found_text | replace: "${phone}", "" %}
{% endif %}

{% assign canceled_text = 'order-tracking.result.canceled_html' | t %}
{% if contact_info.contact_phone != blank %}
    {% assign canceled_text = canceled_text | replace: "${phone}", contact_info.contact_phone %}
{% else %}
    {% assign canceled_text = canceled_text | replace: "${phone}", "" %}
{% endif %}

<script src="{{ 'order-tracking.js' | asset_url }}"></script>
