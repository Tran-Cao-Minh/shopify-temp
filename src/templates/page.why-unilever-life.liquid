<script type="text/javascript">
    $(document).ready(function() {
        {% comment %} Pricing table {% endcomment %}
        function updateHiehgtItem() {
            if($('.section_6_features li').length > 0) {
                $('.section_6_features li').each(function() {
                    const _ = $(this),
                        dataItem = _.data('item_name'),
                        heightItem = _.outerHeight(),
                        itemValue = $(`[data-item_value=${dataItem}]`);

                    if (heightItem && itemValue) {
                        itemValue.height(heightItem - 1);
                    }
                });
            }
        }

        updateHiehgtItem();

        $(window).on('resize',function() {
            updateHiehgtItem();
        });

        {% comment %} Video play {% endcomment %}
        $(document).on('click', '#section_8_play_video', function (e) {
            e.preventDefault();
            const urlYT = $(this).data('url');
            $('#video_section_8').attr("src", urlYT);
            $('.smart_modal-video').show();
        });

        $(document).on('click', '[data-close_video]', function (e) {
            e.preventDefault();
            $('#video_section_8').attr("src", "");
            $('.smart_modal').hide();
        });
    });
</script>
