{%- if section.settings.countdown != blank -%}
  <script src="{{ 'moment-with-locales.js' | asset_url }}"></script>
  <script src="{{ 'moment-timezone.js' | asset_url }}"></script>
  <script src="{{ 'jquery.countdown.min.js' | asset_url }}"></script>
{%- endif -%}
{% include 'assign-locale-prefix' %}
<div id="header_wrap" class="canvasmenu__mobile" data-section-id="{{ section.id }}" data-section-type="header-section" data-header-section>
  <div class="top-header">
    <div class="container-fluid">
      <div class="row align-items-center justify-content-between">
        <div class="col col-left d-flex justify-content-center wrapper-announce">
          {%- if section.settings.message_text != blank -%}
            <span class="top-header__countdown-title hide">{{ section.settings.message_text }}</span>
          {%- endif -%}
          {%- if section.settings.countdown != blank -%}
            <div class="top-header__countdown d-flex hide" data-countdown="{{ section.settings.countdown }}"></div>
          {%- endif -%}
        </div>
        <div class="col col-right d-flex">
          {%- if section.settings.shop_location != blank and section.settings.shop_location_url != blank -%}
            <div class="col-right__menu">
              <a href="{{ section.settings.shop_location_url }}" class="btn-header__top has_icon">{% include 'icon-map' %}{{ section.settings.shop_location }}</a>
            </div>
          {%- endif -%}
          <div class="site-header__currency-selector">
            {%- form 'localization', class: 'selectors-form' -%}
              <input type="hidden" name="csrf" value="{{ "now" | date: "%N" | md5 }}" />
              <div class="selectors-form__item">
                <h2 class="visually-hidden" id="lang-heading">{{ 'general.language.dropdown_label' | t }}</h2>
                <div class="disclosure" data-disclosure-locale>
                  <button aria-controls="lang-list" aria-describedby="lang-heading" aria-expanded="false" class="disclosure__toggle btn-header__top has_icon" data-disclosure-toggle type="button">
                    {%- if form.current_locale.iso_code == 'en' -%}
                      {% include 'icon-en' %}EN
                    {%- else -%}
                      {% include 'icon-th' %}ไทย
                    {%- endif -%}
                    {% include 'icon-chevron-down' %}
                  </button>
                  <ul class="disclosure-list" data-disclosure-list id="lang-list">
                    {%- for locale in form.available_locales -%}
                      {%- if locale.iso_code != form.current_locale.iso_code -%}
                        <li class="disclosure-list__item" data-locale_url="{{ locale.root_url }}">
                          <a class="disclosure-list__option" href="#" lang="{{ locale.iso_code }}" data-value="{{ locale.iso_code }}" data-disclosure-option>
                            {%- if locale.iso_code == 'en' -%}
                              {% include 'icon-en' %}EN
                            {%- else -%}
                              {% include 'icon-th' %}ไทย
                            {%- endif -%}
                          </a>
                        </li>
                      {%- endif -%}
                    {%- endfor -%}
                  </ul>
                  <input type="hidden" name="locale_code" id="LocaleSelector" value="{{ form.current_locale.iso_code }}" data-disclosure-input/>
                </div>
              </div>
            {%- endform -%}
          </div>
        </div>
      </div>
    </div>
  </div>
  <header class="site-header {% if customer %} customer_logged{% endif %}" role="banner">
    <div class="site-header__nav container-fluid">
      <div class="row">
        <div class="col header-left__col">
          {%- if request.page_type == 'index' -%}
            <h1 class="h2 site-header__logo">
          {%- else -%}
            <div class="h2 site-header__logo">
          {%- endif -%}
          {%- if section.settings.logo -%}
            {%- assign img_url = section.settings.logo | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
            <a href="{{ routes.root_url }}" class="site-header__logo-image">
              {% capture logo_alt %}{{ section.settings.logo.alt | default: shop.name }}{% endcapture %}
              <img class="lazyload js{% if section.settings.mobile_logo %} small--hide{% endif %}" src="{{ section.settings.logo | img_url: '300x300' }}" data-src="{{ img_url }}" data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]" data-aspectratio="{{ section.settings.logo.aspect_ratio }}" data-sizes="auto" alt="{{ logo_alt | escape }}">
              {%- if section.settings.mobile_logo -%}
                {%- assign logo_mobile = section.settings.mobile_logo | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                <img class="lazyload js header__logo-mobile" src="{{ section.settings.mobile_logo | img_url: '150x' }}" data-src="{{ logo_mobile }}" data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]" data-aspectratio="{{ section.settings.mobile_logo.aspect_ratio }}" data-sizes="auto" alt="{{ logo_alt | escape }}">
              {%- endif -%}
              <noscript>
                {% capture image_size %}50x
                {% endcapture %}
                <img src="{{ section.settings.logo | img_url: image_size }}" srcset="{{ section.settings.logo | img_url: image_size }} 1x, {{ section.settings.logo | img_url: image_size, scale: 2 }} 2x" alt="{{ section.settings.logo.alt | default: shop.name }}">
              </noscript>
            </a>
          {%- else -%}
            <a class="site-header__logo-link" href="{{ routes.root_url }}">{{ shop.name }}</a>
          {%- endif -%}
          {%- if request.page_type == 'index' -%}
            </h1>
          {%- else -%}
            </div>
          {%- endif -%}
        </div>
        <div class="col header-right__col">
          <div class="row align-items-center">
            <div class="col-md-12 col-lg-5 header_menu-main">
              <nav class="header_navigation-top" role="navigation">
                {%- assign linklist = section.settings.main_linklist -%}
                <ul class="site-nav list--inline">
                  {% for link in linklists[linklist].links %}
                    <li class="{% if link.active %} site-nav--active{% endif %}" data-header_nav_top_item>
                      <a href="{{ link.url }}" class="site-nav__link site-nav__link--main{% if link.active %} site-nav__link--active{% endif %}" {% if link.current %} aria-current="page" {% endif %} data-index="{{ forloop.index }}">
                        <span class="site-nav__label">{{ link.title | escape }}</span>
                        {% include 'icon-menu-down' %}
                      </a>
                    </li>
                  {% endfor %}
                </ul>
              </nav>
            </div>
            <div class="col-md-12 col-lg-7 header-right__action">
              <div class="site-header__icons{% if shop.customer_accounts_enabled %} site-header__icons--plus{% endif %}">
                <div class="site-header__icons-wrapper">
                  {% unless linklists[section.settings.main_linklist] == blank %}
                    <button type="button" id="open-menu-mobile" class="btn--link site-header__icon site-header__menu mobile-nav--open" aria-expanded="false" aria-label="{{ 'layout.navigation.menu' | t }}">
                      {% include 'icon-hamburger' %}
                    </button>
                  {% endunless %}
                  <button class="btn--link site-header__icon site-header__search-toggle js-drawer-open-top" type="button" title="{{ 'general.tooltip.search' | t }}">
                    {% include 'icon-search' %}
                    <span class="icon__fallback-text">{{ 'layout.navigation.search' | t }}</span>
                  </button>
                  {% unless customer %}
                    <span class="site-header__icons-text site-header__icon line-right">
                      {{ section.settings.join_us_text }}
                      <a id="register_link" class="register_link--join-now" href="{{ shop_url }}{{ shop.metafields.route.guest_enroll }}" data-enroll_url>{{ section.settings.join_us_text_button }}</a>
                    </span>
                  {% endunless %}

                  {% if shop.customer_accounts_enabled %}
                    {% if customer %}
                      {%- assign hash_code = customer.id | hmac_sha256: shop.metafields.api.secret -%}
                      <span class="site-header__icons-text site-header__icon line-right">
                        {{ section.settings.join_us_text }}
                        <a data-action="enroll-other" class="register_link--join-now"
                           href="{{ shop_url }}{{ shop.metafields.route.member_enroll }}?customer={{ customer.id }}&hash={{ hash_code }}"
                            data-enroll_url>
                          {{ section.settings.join_us_text_button }}
                        </a>
                      </span>
                      <div class="site-nav--has-dropdown">
                        <a href="{{ routes.account_url }}" class="site-header__icon site-header__account">
                          {% if customer %}
                            {% assign customer_avatar = 'avatar.png' | asset_img_url: '300x' %}
                            {% if customer.metafields.data.personal.avatar_url != blank %}
                              {% assign customer_avatar = customer.metafields.data.personal.avatar_url %}
                            {% endif %}
                            <span class="header__account-avatar"><img src="{{ customer_avatar }}" alt=""></span>
                          {% else %}
                            {% include 'icon-login' %}
                          {% endif %}
                          <span class="site-header__icons-text site-header__account-name">{{ 'layout.customer.welcome' | t }},&nbsp;{{ customer.first_name }}</span>
                        </a>
                        <ul class="site-nav__dropdown site-nav__profile">
                          <li>
                            <a class="site-nav__link site-nav__child-link" href="{{pages['my-order'].url }}">{% render 'icon', name: 'my-order' %}{{ 'layout.navigation.my_order' | t }}</a>
                          </li>
                          <li>
                            <a class="site-nav__link site-nav__child-link" href="{{ pages['order-tracking'].url }}">{% render 'icon', name: 'order-tracking' %}{{ 'layout.navigation.track_and_trace' | t }}</a>
                          </li>
                          <li>
                            <a class="site-nav__link site-nav__child-link" href="{{ pages['my-wishlist'].url }}">{% render 'icon', name: 'my-wishlist' %}{{ 'layout.navigation.wishlist' | t }}</a>
                          </li>
                          <li>
                            <a class="site-nav__link site-nav__child-link" href="{{ routes.account_addresses_url }}">{% render 'icon', name: 'addresses-box' %}{{ 'layout.navigation.address_book' | t }}</a>
                          </li>
                          <li>
                            <a class="site-nav__link site-nav__child-link" href="{{ pages['my-profile'].url }}">{% render 'icon', name: 'my-profile' %}{{ 'layout.navigation.my_profile' | t }}</a>
                          </li>
                          <li>
                            <a class="site-nav__link site-nav__child-link" href="{{ pages['my-wallet'].url }}">{% render 'icon', name: 'my-wallet' %}{{ 'layout.navigation.my_wallet' | t }}</a>
                          </li>
                          <li>
                            <a class="site-nav__link site-nav__child-link" href="{{ routes.account_logout_url }}" data-action="logout">{{ 'layout.navigation.log_out' | t }}, {{ customer.first_name }}</a>
                          </li>
                        </ul>
                      </div>
                    {% else %}
                      {% if request.design_mode %}
                        <a href="{{ routes.account_login_url }}" class="site-header__icon site-header__account">{% include 'icon-login' %}
                        <span class="site-header__icons-text site-header__account-name">{{ 'layout.customer.log_in' | t }}</span></a>
                      {% else %}
                        <form id="offcanvas-login__form" class="form-vertical"
                          name="login" method="GET" action="{{ shop.metafields.route.sso_login }}">
                          {% assign redirect_url = shop.secure_url | append: request.path %}
                          <input type="hidden" name="shop_redirect" value="{{ redirect_url }}" />
                          <input type="hidden" name="csrf" value="{{ "now" | date: "%N" | md5 }}" />
                          <button id="btn-login" type="submit" class="site-header__icon site-header__account open-login_canvas" data-role="btn-login" style="background: transparent;border: none;box-shadow: none;">{% include 'icon-login' %}
                          <span class="site-header__icons-text site-header__account-name">{{ 'layout.customer.log_in' | t }}</span></button>
                        </form>
                      {% endif %}
                    {% endif %}
                  {% endif %}
                  {% unless page.template_suffix == 'checkout' %}
                    <div class="site-header__icon site-header__cart {% if cart.item_count == 0 %} no-item{% endif %}"
                      data-cart_url="{{ routes.cart_url }}">
                      <a href="{{ routes.cart_url }}" class="site-header__icon bag-header__icon" title="{{ 'layout.navigation.bag_tooltip' | t }}">
                        {% include 'icon-cart' %}
                        <span class="icon__fallback-text">{{ 'layout.cart.title' | t }}</span>
                        <div class="site-header__cart-count{% if cart.item_count == 0 %} hide{% endif %}" data-cart-count-bubble id="CartCount">
                          <span data-cart-count>{{ cart.item_count }}</span>
                        </div>
                      </a>
                    </div>
                  {% endunless %}
                </div>
              </div>
            </div>
          </div>
          {% comment %} Submenu {% endcomment %}
          <div class="header-group__submenu">
            <div class="submenu-top submenu__online-shopping justify-content-between{% unless customer %} active{% endunless %}"
              data-submenu__online-shopping>
              <nav class="header_navigation justify-content-between d-flex">
                {% include 'site-nav', linklist: section.settings.menu_online_shop, icon_home: true %}
                {% include 'site-nav', linklist: 'online-shopping-right' %}
              </nav>
            </div>
            <div class="submenu-top submenu__business justify-content-between align-items-center{% if customer %} active{% endif %}"
              data-submenu__business>
              <nav class="header_navigation">
                {% include 'site-nav', linklist: section.settings.menu_business, icon_home: true %}
              </nav>
              <div class="submenu__business-right">
                <ul class="site-nav list--inline">
                  <li>
                    <a class="site-nav__link" href="{{ pages['toolbox'].url }}">
                      {{ 'layout.navigation.toolbox' | t }}
                      {% render 'icon', name: 'arrow_down_2', class_list: 'site-nav__icon' %}
                    </a>
                    {% for block in section.blocks %}
                      {% if block.type == 'toolbox' %}
                        {%- assign toolbox = block.settings -%}
                        <div class="megamenu megamenu__toolbox">
                          <div class="megamenu__wrapper">
                            <div class="container">
                              <div class="row">
                                {% render 'megamenu-toolbox', toolbox: toolbox %}
                              </div>
                            </div>
                          </div>
                        </div>
                      {% endif %}
                    {% endfor %}
                  </li>
                </ul>
              </div>
            </div>
          </div>
          {% comment %} End submenu {% endcomment %}
        </div>
      </div>
    </div>
  </header>
</div>

<div class="sm__menu-mobile sm__menu-online-shopping" data-sm_menu_mobile>
  <div class="menu__mobile-wrapper">
    <div class="menu__mobile-top">
      <span class="close__menu-mobile">{% render 'icon-close' %}</span>
      <div class="back__to-menu">{% render 'icon-back' %}<span>{{ 'layout.navigation.back' | t }}</span></div>
      <div class="multiple__language-mobile"></div>
      <div class="menu__top-content">
        {%- assign hash_code = customer.id | hmac_sha256: shop.metafields.api.secret -%}
        <div class="content__online-shopping">
          {% if customer %}
            <h3 class="top__content-label">{{ 'layout.navigation.top_content_logined_html' | t }}</h3>
            <a href="{{ shop_url }}{{ shop.metafields.route.member_enroll }}?customer={{ customer.id }}&hash={{ hash_code }}" class="d-block btn btn-primary">{{ 'layout.navigation.enroll_for_other' | t }}</a>
          {% else %}
            <h3 class="top__content-label">{{ 'layout.navigation.top_content_online_shopping_not_login_html' | t }}</h3>
            <a href="{{ shop_url }}{{ shop.metafields.route.guest_enroll }}" class="d-block btn btn-primary">{{ 'layout.navigation.start_business' | t }}</a>
          {% endif %}
        </div>
        <div class="content__business">
          {% if customer %}
            <h3 class="top__content-label">{{ 'layout.navigation.top_content_logined_html' | t }}</h3>
            <a href="{{ shop_url }}{{ shop.metafields.route.member_enroll }}?customer={{ customer.id }}&hash={{ hash_code }}" class="d-block btn btn-primary">{{ 'layout.navigation.enroll_for_other' | t }}</a>
          {% else %}
            <h3 class="top__content-label">{{ 'layout.navigation.top_content_business_not_login_html' | t }}</h3>
            <a href="{{ shop_url }}{{ shop.metafields.route.guest_enroll }}" class="d-block btn btn-primary">{{ 'layout.navigation.start_business' | t }}</a>
          {% endif %}
        </div>
        <div class="account__info">
          {% if shop.customer_accounts_enabled %}
            {% if customer %}
              <span><strong>{{ customer.first_name }}</strong> {{ 'layout.customer.is_logged_in' | t }}</span>
            {% else %}
              <span> {{ 'layout.customer.already_account' | t }}</span>
              <form class="form-vertical"
                name="login" method="GET" action="{{ shop.metafields.route.sso_login }}">
                {% assign redirect_url = shop.secure_url | append: request.path %}
                <input type="hidden" name="shop_redirect" value="{{ redirect_url }}" />
                <input type="hidden" name="csrf" value="{{ "now" | date: "%N" | md5 }}" />
                <button type="submit" class="site-header__icon site-header__account open-login_canvas" data-role="btn-login" style="background: transparent;border: none;box-shadow: none;"><span class="open-login_canvas">{{ 'layout.customer.log_in' | t }}</span></button>
              </form>
            {% endif %}
          {% endif %}
        </div>
      </div>
    </div>
    <div class="menu__mobile-content menu__online--shopping" data-mobile_menu_online_shopping>
      <h3 class="parent__menu-title">
        <span>{{ 'layout.navigation.online_shopping' | t }}</span>
        <span class="icon__toggle">{% render 'icon', name: 'arrow_down_3' %}</span>
      </h3>
      <div class="menu__wrapper-content wrapper__online-shopping">
        <ul class="site-nav-mobile">
          {% include 'site-nav-mobile', linklist: section.settings.menu_online_shop, icon_home: true %}
        </ul>
        <div class="menu__online--shopping-right">
          <ul>
            {% for link in linklists['online-shopping-right'].links %}
              {% if forloop.first %}
              <li>
                <a href="{{ link.url }}">
                  {% render 'icon', name: 'representative' %}
                  {{ link.title }}
                </a>
              </li>
              {% else %}
              <li>
                <a href="{{ link.url }}">
                  {% render 'icon', name: 'autoship' %}
                  {{ link.title }}
                </a>
              </li>
              {% endif %}
            {% endfor %}
          </ul>
        </div>
      </div>
    </div>
    <div class="menu__mobile-content menu__bussiness" data-mobile_menu_bussiness>
      <h3 class="parent__menu-title">
        <span>{{ 'layout.navigation.business' | t }}</span>
        <span class="icon__toggle">{% render 'icon', name: 'arrow_down_3' %}</span>
      </h3>
      <div class="menu__wrapper-content wrapper__business">
        <ul class="site-nav-mobile">
          {% include 'site-nav-mobile', linklist: section.settings.menu_business, icon_home: true %}
          <li>
            <div class="toolbox__link-item">
              <a href="{{ pages['toolbox'].url }}">
                {% render 'icon', name: 'toolbox' %}
                <span class="site-nav__label">{{ 'layout.navigation.toolbox' | t }}</span>
              </a>
              <span class="site-nav__icon nav__icon-toolbox">{% include 'icon-arrow-right' %}</span>
            </div>
            <div class="megamenu__toolbox">
              <span class="child__title">
                {% render 'icon', name: 'toolbox' %}
                <span class="site-nav__label">{{ 'layout.navigation.toolbox' | t }}</span>
              </span>
              {% for block in section.blocks %}
                {% if block.type == 'toolbox' %}
                  {% render 'megamenu-toolbox', toolbox: block.settings %}
                {% endif %}
              {% endfor %}
            </div>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>

<script>
  const domSelectors = {
    menu: '.sm__menu-mobile',
    menuWrapper: '.menu__mobile-wrapper',
    buttonOpen: '#open-menu-mobile',
    toggleButton: '.site-nav-mobile li span.site-nav__icon',
    slideButton: '.site-nav-mobile li.level2 a span.icon__arrow',
    langDesktop: '.top-header .site-header__currency-selector',
    langMobile: '.multiple__language-mobile',
    closeButton: '.menu__mobile-top .close__menu-mobile',
    topMenu: '.menu__mobile-top',
    toolboxIcon: '.toolbox__link-item .nav__icon-toolbox'
  }

  window.addEventListener('load', function() {
    const duration = 200;
    const domNodes = {
      menu: document.querySelector(domSelectors.menu),
      menuWrapper: document.querySelector(domSelectors.menuWrapper),
      buttonOpen: document.querySelector(domSelectors.buttonOpen),
      toggleButton: document.querySelectorAll(domSelectors.toggleButton),
      slideButton: document.querySelectorAll(domSelectors.slideButton),
      langDesktop: document.querySelector(domSelectors.langDesktop),
      langMobile: document.querySelector(domSelectors.langMobile),
      closeButton: document.querySelector(domSelectors.closeButton),
      topMenu: document.querySelector(domSelectors.topMenu),
      toolboxIcon: document.querySelector(domSelectors.toolboxIcon)
    }
    
    // Show and Hide language-choice button
    addMobileLang();
    window.addEventListener('resize', addMobileLang);
    function addMobileLang() {
      let scrWidth = window.innerWidth;
      if (scrWidth <= 1170) {
        domNodes.langMobile.appendChild(domNodes.langDesktop);
      } else {
        domNodes.langMobile.innerHTML = '';
        if (!document.querySelector(domSelectors.langDesktop)) {
          document.querySelector('.top-header .col-right').appendChild(domNodes.langDesktop);
        }
      }
    }

    domNodes.menu.style.overflow = 'hidden';
    domNodes.menu.style.display = 'none';
    domNodes.menu.style.background = 'rgba(0,0,0,0)';
    domNodes.menuWrapper.style.transform = 'translateX(111%)';

    Array.prototype.forEach.call(domNodes.toggleButton, function(elm) {
      elm.addEventListener('click', function(e) {
        let targetElm = $(this).parent().next();
        $(targetElm).slideToggle();

        let elLi = $(this).parents('li').eq(0);
        
        if (elLi.hasClass('active')) {
          elLi.removeClass('active');
        }else {
          // Find li has class active to remove
          let elLiActive = elLi.parent('ul').find('li.active'),
              elUlAactive = elLiActive.find('> ul');
          elLiActive.removeClass('active');

          if(elUlAactive.length > 0) {
            elUlAactive.hide();
          }else {
            elLiActive.find('> .sub__menu--wrapper').hide();
          }

          elLi.addClass('active');
        }
      })
    })
    domNodes.toolboxIcon?.addEventListener('click', function(e) {
      e.preventDefault();
      let targetElm = $(this).parent().next();
      targetElm.css('display', 'block');
      $('.menu__mobile-wrapper').animate({ scrollTop: 0 }, 200);
      $('.menu__top-content').slideUp(200);
      $('.close__menu-mobile').hide();
      $('.back__to-menu').show();
      $('.menu__mobile-wrapper').addClass('has__last-level');
      setTimeout(function() {
        targetElm.css('transform', 'translateX(0)');
        targetElm.css('transitionDuration', '.2s');
        $('.menu__mobile-content.active').css('height', 'calc(100% - 100px)');
        $('.menu__mobile-content.active').css('overflow', 'hidden');
      }, duration)
      $('.back__to-menu').on('click', function() {
        targetElm.css('transform', 'translateX(100%)');
        $('.menu__mobile-content.active').css('height', 'auto');
        $('.menu__mobile-content.active').css('overflow', '');
        $('.menu__mobile-wrapper').removeClass('has__last-level');
        $(this).hide();
        $('.menu__top-content').slideDown(200);
        $('.close__menu-mobile').show();
        setTimeout(function() {
          targetElm.css('display', 'none');
        }, duration)
      });
    })

    $(".menu__online--shopping .icon__toggle").click(function(){
        $('.back__to-menu').trigger('click');
        $(".menu__wrapper-content").not(".wrapper__online-shopping").slideUp();
        $(".wrapper__online-shopping").slideToggle();
        $(".menu__online--shopping").toggleClass('active');
        $(".menu__bussiness").removeClass('active');
        $('.menu__bussiness').css('height', '');
        $('.menu__mobile-content').css('height', '');
        if ($('.menu__mobile-wrapper').hasClass('has__last-level')) {
          $('.menu__mobile-content.active').css('height', 'calc(100% - 100px)');
          $('.menu__mobile-content.active').css('overflow', 'auto');
        }
    });

    $("#register_link").click(function(){
      var ev = {};
      ev.eventInfo={
        'type':ctConstants.trackEvent,
        'eventAction': ctConstants.RegistrationStart,
        'eventLabel' : window.location.href,
        'eventValue' :1
      };
      ev.category ={'primaryCategory':ctConstants.other};
      ev.subcategory = 'Lead';
      digitalData.event.push(ev);
    });

    $(".menu__bussiness .icon__toggle").click(function(){
        $('.back__to-menu').trigger('click');
        $(".menu__wrapper-content").not(".wrapper__business").slideUp();
        $(".wrapper__business").slideToggle();
        $(".menu__bussiness").toggleClass('active');
        $(".menu__online--shopping").removeClass('active');
        $('.menu__online--shopping').css('height', '');
        $('.menu__mobile-content').css('height', '');
        if ($('.menu__mobile-wrapper').hasClass('has__last-level')) {
          $('.menu__mobile-content.active').css('height', 'calc(100% - 100px)');
          $('.menu__mobile-content.active').css('overflow', 'auto');
        }
    });

    Array.prototype.forEach.call(domNodes.slideButton, function(elm) {
      elm.addEventListener('click', function(e) {
        e.preventDefault();
        let targetElm = $(this).parent().next();
        targetElm.css('display', 'block');
        $('.menu__mobile-wrapper').animate({ scrollTop: 0 }, 200);
        $('.menu__top-content').slideUp(200);
        $('.close__menu-mobile').hide();
        $('.back__to-menu').show();
        $('.menu__mobile-wrapper').addClass('has__last-level');
        setTimeout(function() {
          targetElm.css('transform', 'translateX(0)');
          targetElm.css('transitionDuration', '.2s');
          targetElm.css('height', 'calc(100vh - 155px)');
          targetElm.css('overflow', 'auto');
          $('.menu__mobile-content.active').css('height', 'calc(100% - 100px)');
          $('.menu__mobile-content.active').css('overflow', 'hidden');
        }, duration)
        $('.back__to-menu').on('click', function() {
          targetElm.css('transform', 'translateX(100%)');
          targetElm.css('height', '');
          targetElm.css('overflow', '');
          $('.menu__mobile-content.active').css('height', 'auto');
          $('.menu__mobile-content.active').css('overflow', '');
          $(this).hide();
          $('.menu__top-content').slideDown(200);
          $('.close__menu-mobile').show();
          $('.menu__mobile-wrapper').removeClass('has__last-level');
          setTimeout(function() {
            targetElm.css('display', 'none');
          }, duration)
        });
      })
    })

    domNodes.buttonOpen.addEventListener('click', function(e) {
      openMenuMobile();
    })

    document.addEventListener('click', function(e) {
      if (e.target === domNodes.menu) {
        closeMenuMobile();
      }
    })

    domNodes.closeButton.addEventListener('click', function() {
      closeMenuMobile();
    })

    function openMenuMobile() {
      domNodes.menu.style.overflow = '';
      domNodes.menu.style.display = 'block';
      domNodes.menu.style.background = 'rgba(0,0,0,0.3)';
      document.documentElement.style.overflow = 'hidden';
      setTimeout(function() {
        domNodes.menuWrapper.style.transform = 'translateX(11%)'
      }, duration)
    }
    function closeMenuMobile() {
      domNodes.menuWrapper.style.transform = 'translateX(111%)'
      setTimeout(function() {
        domNodes.menu.style.overflow = 'hidden';
        domNodes.menu.style.display = 'none';
        document.documentElement.style.overflow = '';
      }, duration)
    }
  });

  $('#lang-list .disclosure-list__option').click(function (event) {
    // event.preventDefault();
    const LangSelected = $(this).text().trim();
    //console.log(LangSelected);
    var ev = {};
    ev.eventInfo={
      'type':ctConstants.trackEvent,
      'eventAction': ctConstants.languageSelector,
      'eventLabel' : LangSelected,
      'eventValue' :1                                                                                                                                                                                                                                                                                                                                              };
    ev.category ={'primaryCategory':ctConstants.custom};
    ev.subcategory = 'Read';
    digitalData.event.push(ev);

  });
</script>

