<script>
  $(document).ready(function($) {
    // $('.sm__megamenu-tabs').find('.tabcontent .sm__megamenu-lists > li.megamenu > a').addClass('active');
    $(".sm__megamenu-tabs ul > li > a.tablinks").hover(function() {
      $(this).parents('.sm__megamenu-tabs').find('.tabcontent').hide();
      $(this).parents('.sm__megamenu-tabs').find('.tabcontent').removeClass('active');
      $(this).parents('.sm__megamenu-tabs-header').find('li > a.tablinks').removeClass('active');
      $(this).addClass("active");
      let selected_tab = $(this).attr("data-tab");
      $(selected_tab).addClass('active');
      $(selected_tab).show();
      return false;
    });

    // $(".sm-megamenu-submenu > li > div").first().parent().addClass('active')
    $(".sm-megamenu-submenu > li").hover(function() {
      $('.sm-megamenu-submenu > li').removeClass('active');
      $(this).addClass("active");
    });

    $(document).on('click', function(e) {
      let megamenu = $('div.megamenu');
      if (e.target === megamenu) {
        $('.header-group__submenu .submenu-top .site-nav > li').removeClass('active');
      }
    });

    $(".header-group__submenu .submenu-top .site-nav > li").hover(
      function () {
        $(this).toggleClass('active');
      },
      function () {
        $(this).toggleClass('active');
      }
    );
  });
</script>